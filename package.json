{
  "name": "@fjedi/react-router-helpers",
  "version": "4.0.5",
  "description": "A set of helpers for react-router",
  "main": "lib/index.js",
  "browser": "lib/browser",
  "files": [
    "lib"
  ],
  "private": false,
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watchAll",
    "clean": "rimraf lib",
    "coverage": "jest --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "build": "rollup -c",
    "start": "rollup -c -w",
    "format": "prettier --write \"src/**/*.tsx\"",
    "prepare": "npm run build && husky install",
    "prepublishOnly": "npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fjedi/react-router-helpers.git"
  },
  "keywords": [
    "fjedi",
    "react",
    "react-router",
    "typescript"
  ],
  "author": "Alexander Radyushin <r.alex.andr@ya.ru> (https://fjedi.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fjedi/react-router-helpers/issues"
  },
  "homepage": "https://github.com/fjedi/react-router-helpers#readme",
  "devDependencies": {
    "@rollup/plugin-commonjs": "22.0.2",
    "@rollup/plugin-node-resolve": "14.0.0",
    "@rollup/plugin-replace": "4.0.0",
    "@types/jest": "29.0.0",
    "@types/node": "18.7.15",
    "@types/react": "18.0.18",
    "@typescript-eslint/eslint-plugin": "5.36.2",
    "@typescript-eslint/parser": "5.36.2",
    "babel-core": "6.26.3",
    "babel-runtime": "6.26.0",
    "eslint": "8.22.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-prettier": "4.2.1",
    "husky": "8.0.1",
    "jest": "28.1.3",
    "lint-staged": "13.0.3",
    "prettier": "2.7.1",
    "prop-types": "15.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "rimraf": "3.0.2",
    "rollup": "2.79.0",
    "rollup-plugin-commonjs": "10.1.0",
    "rollup-plugin-node-polyfills": "0.2.1",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-peer-deps-external": "2.2.4",
    "rollup-plugin-terser": "7.0.2",
    "rollup-plugin-typescript2": "0.33.0",
    "ts-jest": "28.0.8",
    "typescript": "4.8.2"
  },
  "peerDependencies": {
    "prop-types": ">=15.8.1",
    "react": ">=18.2.0",
    "react-dom": ">=18.2.0"
  },
  "lint-staged": {
    "*.(js|jsx|ts|tsx)": [
      "npm run lint:fix",
      "git add"
    ]
  },
  "typings": "./lib/index.d.ts",
  "dependencies": {
    "history": "5.3.0",
    "react-router": "6.3.0",
    "react-router-dom": "6.3.0"
  }
}
