{
  "name": "@fjedi/react-router-helpers",
  "version": "1.12.0",
  "description": "A set of helpers for react-router",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "private": false,
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watchAll",
    "clean": "rimraf lib",
    "coverage": "jest --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "build": "rollup -c",
    "start": "rollup -c -w",
    "format": "prettier --write \"src/**/*.tsx\"",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fjedi/react-router-helpers.git"
  },
  "keywords": [
    "fjedi",
    "react",
    "react-router",
    "typescript"
  ],
  "author": "Alexander Radyushin <r.alex.andr@ya.ru> (https://fjedi.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fjedi/react-router-helpers/issues"
  },
  "homepage": "https://github.com/fjedi/react-router-helpers#readme",
  "dependencies": {},
  "devDependencies": {
    "@types/expect": "24.3.0",
    "@types/jest": "26.0.23",
    "@types/node": "14.14.43",
    "@types/react": "17.0.5",
    "@types/react-router-dom": "5.1.7",
    "@typescript-eslint/eslint-plugin": "4.23.0",
    "@typescript-eslint/parser": "4.23.0",
    "babel-core": "6.26.3",
    "babel-runtime": "6.26.0",
    "eslint": "7.25.0",
    "eslint-config-airbnb-typescript": "12.3.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-prettier": "3.4.0",
    "husky": "4.3.8",
    "jest": "26.6.3",
    "lint-staged": "10.5.4",
    "prettier": "2.2.1",
    "prop-types": "15.7.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-router-dom": "5.2.0",
    "rimraf": "3.0.2",
    "rollup": "2.47.0",
    "rollup-plugin-commonjs": "10.1.0",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-peer-deps-external": "2.2.4",
    "rollup-plugin-typescript2": "0.30.0",
    "ts-jest": "26.5.6",
    "typescript": "4.2.4"
  },
  "peerDependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "prop-types": "^15.7.2"
  },
  "lint-staged": {
    "*.(js|jsx|ts|tsx)": [
      "npm run lint:fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
