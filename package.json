{
  "name": "@fjedi/react-router-helpers",
  "version": "2.0.1",
  "description": "A set of helpers for react-router",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "private": false,
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watchAll",
    "clean": "rimraf lib",
    "coverage": "jest --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "build": "rollup -c",
    "start": "rollup -c -w",
    "format": "prettier --write \"src/**/*.tsx\"",
    "prepare": "npm run build && husky install",
    "prepublishOnly": "npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fjedi/react-router-helpers.git"
  },
  "keywords": [
    "fjedi",
    "react",
    "react-router",
    "typescript"
  ],
  "author": "Alexander Radyushin <r.alex.andr@ya.ru> (https://fjedi.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fjedi/react-router-helpers/issues"
  },
  "homepage": "https://github.com/fjedi/react-router-helpers#readme",
  "devDependencies": {
    "@types/expect": "24.3.0",
    "@types/jest": "28.1.1",
    "@types/node": "17.0.41",
    "@types/react": "17.0.45",
    "@types/react-router-dom": "5.3.3",
    "@typescript-eslint/eslint-plugin": "5.27.1",
    "@typescript-eslint/parser": "5.27.1",
    "babel-core": "6.26.3",
    "babel-runtime": "6.26.0",
    "eslint": "8.17.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-prettier": "4.0.0",
    "husky": "8.0.1",
    "jest": "28.1.1",
    "lint-staged": "13.0.1",
    "prettier": "2.6.2",
    "prop-types": "15.7.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-router-dom": "5.3.0",
    "rimraf": "3.0.2",
    "rollup": "2.75.6",
    "rollup-plugin-commonjs": "10.1.0",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-peer-deps-external": "2.2.4",
    "rollup-plugin-typescript2": "0.32.1",
    "ts-jest": "28.0.4",
    "typescript": "4.7.3"
  },
  "peerDependencies": {
    "prop-types": ">=15.8.1",
    "react": ">=16.9.0",
    "react-dom": ">=16.9.0",
    "react-router-dom": ">=5.3.0"
  },
  "lint-staged": {
    "*.(js|jsx|ts|tsx)": [
      "npm run lint:fix",
      "git add"
    ]
  }
}
